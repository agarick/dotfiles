  cm = commit
  co = checkout
  cu = !git commit --date="$(date --utc +%Y-%m-%dT%H:%M:%S%z)"
  df = diff
  lo = log --graph --pretty=format:'%C(cyan)%h %C(yellow)%ad %C(green)%an %C(reset)%s%d'
  nr = name-rev
  rb = rebase
  rl = reflog
  s  = status --branch --short
  st = status --branch
